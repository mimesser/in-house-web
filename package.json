{
  "name": "in-house",
  "main": "server/index.js",
  "scripts": {
    "lint": "eslint --ext .js src server/index.js",
    "dev": "cross-env MODE=dev npm run lint && npm run clean-cache && nodemon server/index.js",
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "build": "npm run clean-cache && next build ./src",
    "start": "cross-env MODE=production NODE_ENV=production node server/index.js",
    "prestart": "npm run build",
    "test": "jest --env=jsdom --no-cache --colors --coverage test",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "storybook": "start-storybook -p 9001 -c config/storybook"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "clean-tag": "^3.1.1",
    "isomorphic-unfetch": "^3.0.0",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.11",
    "next": "^8.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0",
    "styled-icons": "^7.10.0",
    "styled-system": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addons": "^5.0.10",
    "@storybook/react": "^5.0.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.5",
    "babel-preset-airbnb": "^3.2.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "faker": "^4.1.0",
    "jest-cli": "^23.5.0",
    "jsdom": "^11.11.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "opn-cli": "^3.1.0",
    "nodemon": "^1.18.10",
    "prettier": "1.16.4",
    "rimraf": "^2.6.3",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.2.3",
    "webpack": "^4.30.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn build && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.j{s,sx}": [
      "eslint --fix --quiet",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ]
}
