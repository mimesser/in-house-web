{
   "name": "in-house",
   "main": "server/index.js",
   "scripts": {
      "lint": "eslint --ext .js src server/index.js",
      "local-staging": "cross-env MODE=localStaging npm run start:watch",
      "local": "cross-env MODE=local npm run start:watch",
      "start:watch": "npm run build && nodemon server/index.js",
      "clean-next": "rimraf ./src/.next",
      "build": "npm run clean-next && next build ./src",
      "start": "cross-env NODE_ENV=production node server/index.js",
      "postinstall": "npm run build",
      "test": "jest --env=jsdom --no-cache --colors --coverage test",
      "test:watch": "npm test -- --watch",
      "coverage": "npm test -- --coverage",
      "postcoverage": "opn coverage/lcov-report/index.html",
      "storybook": "start-storybook -p 9001 -c config/storybook"
   },
   "dependencies": {
      "axios": "0.18.0",
      "date-fns": "1.30.1",
      "dotenv": "8.0.0",
      "es6-promise": "4.1.1",
      "koa": "2.7.0",
      "koa-basic-auth": "4.0.0",
      "koa-router": "7.4.0",
      "lodash": "4.17.11",
      "next": "8.1.0",
      "next-redux-saga": "4.0.0",
      "next-redux-wrapper": "2.0.0",
      "normalizr": "3.3.0",
      "polished": "3.3.2",
      "prop-types": "15.7.2",
      "react": "16.8.6",
      "react-dom": "16.8.6",
      "react-redux": "5.0.7",
      "react-sidebar": "3.0.2",
      "react-swipe": "6.0.4",
      "redux": "4.0.1",
      "redux-saga": "1.0.1",
      "reselect": "4.0.0",
      "styled-components": "4.2.0",
      "styled-icons": "8.0.0",
      "styled-normalize": "8.0.6",
      "swipe-js-iso": "2.1.5"
   },
   "devDependencies": {
      "@babel/core": "7.4.0",
      "@storybook/addon-actions": "5.0.10",
      "@storybook/addons": "5.0.10",
      "@storybook/react": "5.0.10",
      "babel-core": "7.0.0-bridge.0",
      "babel-eslint": "10.0.1",
      "babel-jest": "23.4.2",
      "babel-loader": "8.0.5",
      "babel-plugin-styled-components": "1.10.0",
      "babel-preset-airbnb": "3.2.1",
      "cross-env": "5.2.0",
      "enzyme": "3.9.0",
      "enzyme-adapter-react-16": "1.12.1",
      "eslint": "5.15.1",
      "eslint-config-airbnb": "17.1.0",
      "eslint-config-prettier": "4.1.0",
      "eslint-import-resolver-alias": "1.1.2",
      "eslint-plugin-babel": "5.3.0",
      "eslint-plugin-import": "2.16.0",
      "eslint-plugin-jsx-a11y": "6.2.1",
      "eslint-plugin-prettier": "3.0.1",
      "eslint-plugin-react": "7.12.4",
      "husky": "1.3.1",
      "jest-cli": "23.5.0",
      "jsdom": "11.11.0",
      "lint-staged": "8.1.5",
      "nodemon": "1.18.10",
      "opn-cli": "3.1.0",
      "prettier": "1.16.4",
      "react-test-renderer": "16.0.0",
      "redux-devtools-extension": "2.13.2",
      "redux-mock-store": "1.2.3",
      "rimraf": "2.6.3",
      "webpack": "4.30.0"
   },
   "husky": {
      "hooks": {
         "pre-commit": "npm run build && lint-staged"
      }
   },
   "lint-staged": {
      "**/*.j{s,sx}": [
         "eslint --fix --quiet",
         "prettier --write",
         "git add"
      ]
   },
   "browserslist": [
      ">0.2%",
      "not dead",
      "not ie <= 9",
      "not op_mini all",
      "not samsung 4"
   ]
}
