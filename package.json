{
    "name": "in-house",
    "private": true,
    "main": "server/index.js",
    "version": "1.0.0-alpha+111021.0937",
    "scripts": {
        "build:ci": "cross-env npm run clean && npm run build && copyfiles -a \"src/.next/**/*\" \"src/static/**/*\" \"src/public/**/*\" server/*.* package.json package-lock.json next.config.js build",
        "build": "cross-env next build",
        "clean": "cross-env rimraf ./build",
        "clean-sentry-config": "node .sentryclirc.resetter.js && git add .sentryclirc",
        "coverage": "npm test -- --coverage",
        "lint": "eslint --ext .js src server/index.js",
        "lint-fix": "eslint --fix --ext .js src && eslint --fix server/index.js && prettier --config .prettierrc.js --write src",
        "next:dev": "next dev",
        "prepare": "is-ci || husky install",
        "postcoverage": "open-cli coverage/lcov-report/index.html",
        "server:watch": "nodemon server/index.js",
        "start:dev": "npm run lint-fix && node .sentryclirc.enhancer.js && npm run start:prod",
        "start:prod": "npm run clean && npm run build && npm run server:watch",
        "start": "npm run start:prod",
        "storybook": "start-storybook -p 9001 -c config/storybook",
        "test:watch": "npm test -- --watch",
        "test": "jest --env=jsdom --no-cache --colors test",
        "docs": "jsdoc -c config/jsdoc/jsdoc.json"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=7.0.0"
    },
    "dependencies": {
        "@koa/router": "^9.0.1",
        "@next/eslint-plugin-next": "^11.1.2",
        "@sentry/nextjs": "^6.14.1",
        "axios": "^0.24.0",
        "axios-mock-adapter": "1.18.1",
        "babel-plugin-styled-components": "^1.13.2",
        "copyfiles": "^2.4.1",
        "cross-env": "7.0.2",
        "husky": "^7.0.4",
        "is-ci": "github:watson/is-ci",
        "koa": "2.12.0",
        "koa-basic-auth": "4.0.0",
        "koa-slow": "^2.1.0",
        "lodash": "^4.17.20",
        "moment": "^2.26.0",
        "next": "^12.0.3",
        "next-redux-wrapper": "^7.0.5",
        "nodemon": "^2.0.12",
        "polished": "3.6.4",
        "raw-loader": "^4.0.1",
        "react": "^17.0.2",
        "react-countdown-circle-timer": "^2.5.3",
        "react-dom": "^17.0.2",
        "react-ga": "^3.3.0",
        "react-helmet": "^6.0.0",
        "react-hotjar": "^3.0.1",
        "react-popper-tooltip": "^3.1.1",
        "react-redux": "^7.2.5",
        "react-select": "^5.0.0",
        "react-sticky": "^6.0.3",
        "react-swipeable": "^6.2.0",
        "react-test-renderer": "^17.0.2",
        "react-transition-group": "4.4.1",
        "redux": "4.0.5",
        "redux-devtools-extension": "^2.13.9",
        "redux-saga": "1.1.3",
        "reselect": "4.0.0",
        "rimraf": "3.0.2",
        "rooks": "^5.7.1",
        "styled-components": "5.3.0"
    },
    "devDependencies": {
        "@babel/core": "7.13.0",
        "@storybook/addon-actions": "^6.3.8",
        "@storybook/addons": "^6.3.8",
        "@storybook/react": "^6.3.8",
        "babel-eslint": "10.1.0",
        "babel-jest": "26.0.1",
        "babel-loader": "8.1.0",
        "enzyme": "3.11.0",
        "eslint": "7.2.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-prettier": "6.11.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-babel": "5.3.0",
        "eslint-plugin-import": "2.23.4",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-prettier": "3.1.3",
        "eslint-plugin-react": "7.21.5",
        "jest-cli": "26.0.1",
        "jsdoc": "^3.6.7",
        "lint-staged": "10.2.7",
        "npm-run-all": "^4.1.5",
        "open-cli": "6.0.1",
        "prettier": "2.0.5",
        "prop-types": "^15.8.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run clean-sentry-config && lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{js,jsx}": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 9",
        "not op_mini all",
        "not samsung 4"
    ]
}
