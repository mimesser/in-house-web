{
  "name": "in-house",
  "private": true,
  "main": "server/index.js",
  "scripts": {
    "lint": "eslint --ext .js src server/index.js",
    "local-dev": "cross-env MODE=dev npm run start:watch",
    "local": "cross-env MODE=local npm run start:watch",
    "next:dev": "next dev ./src",
    "server:watch": "nodemon server/index.js",
    "start:watch": "npm run server:watch && npm run next:dev",
    "clean": "rimraf ./build",
    "build": "next build ./src",
    "build:ci": "npm run clean && npm run build && copyfiles -a \"src/.next/**/*\" \"src/static/**/*\" \"src/public/**/*\" server/*.* package.json package-lock.json next.config.js build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:production": "cross-env NODE_ENV=production MODE=production node server/index.js",
    "test": "jest --env=jsdom --no-cache --colors test",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "storybook": "start-storybook -p 9001 -c config/storybook"
  },
  "dependencies": {
    "axios": "0.19.2",
    "axios-mock-adapter": "1.18.1",
    "bowser": "2.9.0",
    "cross-env": "7.0.2",
    "date-fns": "2.11.1",
    "koa": "2.11.0",
    "koa-basic-auth": "4.0.0",
    "koa-router": "8.0.8",
    "lodash": "4.17.15",
    "next": "9.3.4",
    "next-redux-saga": "4.1.2",
    "next-redux-wrapper": "5.0.0",
    "polished": "3.5.1",
    "raw-loader": "^4.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-popper-tooltip": "2.11.0",
    "react-redux": "7.2.0",
    "react-select": "3.1.0",
    "react-transition-group": "4.3.0",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "styled-components": "5.0.1",
    "swipe-js-iso": "2.1.5"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/react": "5.3.18",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.2.4",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "1.10.7",
    "babel-preset-airbnb": "4.4.0",
    "copyfiles": "2.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "husky": "4.2.3",
    "jest-cli": "25.2.4",
    "jsdom": "16.2.2",
    "lint-staged": "10.1.1",
    "nodemon": "2.0.2",
    "open-cli": "6.0.1",
    "prettier": "2.0.2",
    "react-test-renderer": "16.13.1",
    "redux-devtools-extension": "2.13.8",
    "rimraf": "3.0.2",
    "webpack": "4.42.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.j{s,sx}": [
      "eslint --fix --quiet",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all",
    "not samsung 4"
  ]
}
