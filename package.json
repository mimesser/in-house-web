{
  "name": "in-house",
  "private": true,
  "main": "server/index.js",
  "scripts": {
    "lint": "eslint --ext .js src server/index.js",
    "local-dev": "cross-env MODE=dev npm run start:watch",
    "local": "cross-env MODE=local npm run start:watch",
    "next:dev": "next dev ./src",
    "server:watch": "nodemon server/index.js",
    "start:watch": "npm run server:watch && npm run next:dev",
    "clean": "rimraf ./build",
    "build": "next build ./src",
    "build:ci": "npm run clean && npm run build && copyfiles -a \"src/.next/**/*\" \"src/static/**/*\" \"src/public/**/*\" server/*.* package.json package-lock.json next.config.js build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:production": "cross-env NODE_ENV=production MODE=production node server/index.js",
    "test": "jest --env=jsdom --no-cache --colors test",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "storybook": "start-storybook -p 9001 -c config/storybook"
  },
  "dependencies": {
    "axios": "0.19.0",
    "axios-mock-adapter": "^1.17.0",
    "bowser": "2.6.0",
    "cross-env": "5.2.0",
    "date-fns": "1.30.1",
    "koa": "2.7.0",
    "koa-basic-auth": "4.0.0",
    "koa-router": "7.4.0",
    "lodash": "4.17.15",
    "next": "9.0.3",
    "next-redux-saga": "4.0.2",
    "next-redux-wrapper": "2.0.0",
    "polished": "3.4.1",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.0",
    "react-select": "3.0.4",
    "react-swipe": "6.0.4",
    "react-transition-group": "4.3.0",
    "redux": "4.0.4",
    "redux-saga": "1.0.5",
    "reselect": "4.0.0",
    "styled-components": "4.3.2",
    "styled-icons": "8.3.0",
    "swipe-js-iso": "2.1.5"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@storybook/addon-actions": "5.1.10",
    "@storybook/addons": "5.1.10",
    "@storybook/react": "5.1.10",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.6",
    "babel-preset-airbnb": "4.0.1",
    "copyfiles": "2.1.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "husky": "3.0.3",
    "jest-cli": "24.8.0",
    "jsdom": "15.1.1",
    "lint-staged": "9.2.1",
    "nodemon": "1.19.1",
    "open-cli": "5.0.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.9.0",
    "redux-devtools-extension": "2.13.8",
    "rimraf": "2.6.3",
    "webpack": "4.39.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.j{s,sx}": [
      "eslint --fix --quiet",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all",
    "not samsung 4"
  ]
}
