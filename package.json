{
    "name": "in-house",
    "private": true,
    "main": "server/index.js",
    "version": "1.2.11",
    "scripts": {
        "build:ci": "cross-env npm run clean && npm run build && copyfiles -a \"src/.next/**/*\" \"src/static/**/*\" \"src/public/**/*\" server/*.* package.json package-lock.json next.config.js build",
        "build": "cross-env next build ./src",
        "clean": "cross-env rimraf ./build",
        "coverage": "npm test -- --coverage",
        "lint": "eslint --ext .js src server/index.js",
        "next:dev": "next dev ./src",
        "postcoverage": "open-cli coverage/lcov-report/index.html",
        "server:watch": "nodemon server/index.js",
        "start:dev": "npm run next:dev",
        "start:prod": "npm run clean && npm run build && npm run server:watch",
        "start": "npm run start:prod",
        "storybook": "start-storybook -p 9001 -c config/storybook",
        "test:watch": "npm test -- --watch",
        "test": "jest --env=jsdom --no-cache --colors test"
    },
    "dependencies": {
        "@koa/router": "^9.0.1",
        "@n8tb1t/use-scroll-position": "^1.0.43",
        "@react-hook/window-size": "^3.0.7",
        "axios": "0.19.2",
        "axios-mock-adapter": "1.18.1",
        "copyfiles": "^2.4.1",
        "cross-env": "7.0.2",
        "dotenv": "^8.2.0",
        "koa": "2.12.0",
        "koa-basic-auth": "4.0.0",
        "koa-slow": "^2.1.0",
        "lodash": "^4.17.20",
        "moment": "^2.26.0",
        "next": "9.5.5",
        "next-redux-saga": "4.1.2",
        "next-redux-wrapper": "6.0.0",
        "polished": "3.6.4",
        "raw-loader": "^4.0.1",
        "react": "16.13.1",
        "react-countdown-circle-timer": "^2.5.3",
        "react-dom": "16.13.1",
        "react-ga": "^2.7.0",
        "react-helmet": "^6.0.0",
        "react-popper-tooltip": "2.11.1",
        "react-redux": "7.2.0",
        "react-select": "3.1.0",
        "react-sticky": "^6.0.3",
        "react-swipeable": "^5.5.1",
        "react-transition-group": "4.4.1",
        "redux": "4.0.5",
        "redux-devtools-extension": "^2.13.9",
        "redux-saga": "1.1.3",
        "reselect": "4.0.0",
        "rimraf": "3.0.2",
        "styled-components": "5.1.1"
    },
    "devDependencies": {
        "@babel/core": "7.13.0",
        "@storybook/addon-actions": "5.3.19",
        "@storybook/addons": "5.3.19",
        "@storybook/react": "5.3.19",
        "babel-eslint": "10.1.0",
        "babel-jest": "26.0.1",
        "babel-loader": "8.1.0",
        "babel-plugin-styled-components": "1.10.7",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.6",
        "eslint": "7.2.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-prettier": "6.11.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-babel": "5.3.0",
        "eslint-plugin-import": "2.23.4",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-prettier": "3.1.3",
        "eslint-plugin-react": "7.21.5",
        "husky": "4.2.5",
        "jest-cli": "26.0.1",
        "lint-staged": "10.2.7",
        "nodemon": "2.0.4",
        "npm-run-all": "^4.1.5",
        "open-cli": "6.0.1",
        "prettier": "2.0.5",
        "react-test-renderer": "16.13.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{js,jsx}": [
            "eslint --fix --quiet",
            "prettier --write",
            "git add"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 9",
        "not op_mini all",
        "not samsung 4"
    ]
}
