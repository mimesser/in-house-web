{
   "extends": ["tslint:recommended", "tslint-config-standard", "tslint-react"],
   "rules": {
      "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
      "semicolon": [true],
      "no-var-requires": [false],
      "ter-indent": [true, 3],
      "no-console": [false]
   }
}